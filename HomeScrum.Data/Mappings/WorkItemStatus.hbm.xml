<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="HomeScrum.Data"
                   namespace="HomeScrum.Data.Domain">

  <class name="WorkItemStatus"
         table="WorkItemStatuses">
    <id name="Id">
      <generator class="guid"></generator>
    </id>
    <property name="Name"/>
    <property name="Description"/>
    <property name="StatusCd"/>
    <property name="IsOpenStatus"
              type="YesNo"/>
    <property name="IsPredefined"
              type="YesNo"/>
    <property name="SortSequence"/>
  </class>

</hibernate-mapping>