@model HomeScrum.Web.Models.Admin.SprintStatusViewModel

@Html.Partial( "_DomainObjectBaseDetails", Model )

<fieldset>
   <legend class="hidden">SprintStatus</legend>

   <dl>
      <dt>Category</dt>
      <dd>@Html.DisplayFor( model => model.Category )</dd>
      <dt>@Html.DisplayNameFor( model => model.CanAddBacklogItems )</dt>
      <dd>@(Model.CanAddBacklogItems ? "Yes" : "No")</dd>
      <dt>@Html.DisplayNameFor( model => model.CanAddTaskListItems )</dt>
      <dd>@(Model.CanAddTaskListItems ? "Yes" : "No")</dd>
      <dt>@Html.DisplayNameFor( model => model.AllowUse )</dt>
      <dd>@(Model.AllowUse ? "Yes" : "No")</dd>
   </dl>
</fieldset>
