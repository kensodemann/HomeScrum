@model HomeScrum.Web.Models.Base.DomainObjectViewModel


@{
    ViewBag.EditorButtonLabel = "Save";
}

<h2>@ViewBag.Title</h2>

@Html.Partial( "_Editor", Model )

<div>
    @Html.ActionLink( "Cancel", Model.CallingAction ?? "Index",
       (Model.CallingId != default( Guid )) ? new { id = Model.CallingId.ToString() } : null )
</div>

@section Scripts {
    @Scripts.Render( "~/bundles/jqueryval" )
}
