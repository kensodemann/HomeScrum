@model IEnumerable<HomeScrum.Web.Models.Sprints.SprintIndexViewModel>

<h2>@Html.ViewBag.Title</h2>

<table class="dataTableList table table-striped table-bordered">
   <thead class="ListHead">
      <tr>
         <th>
            @Html.DisplayNameFor( model => model.ProjectName )
         </th>
         <th>
            @Html.DisplayNameFor( model => model.Name )
         </th>
         <th>
            @Html.DisplayNameFor( model => model.StatusName )
         </th>
         <th>
            @Html.DisplayNameFor( model => model.StartDate )
         </th>
         <th>
            @Html.DisplayNameFor( model => model.EndDate )
         </th>
      </tr>
   </thead>

   <tbody class="ListBody">
      @foreach (var item in Model)
      {
         <tr>
            <td>
               @Html.DisplayFor( modelItem => item.ProjectName )
            </td>
            <td>
               @Html.ActionLink( item.Name, "Edit", new { id = item.Id } )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.StatusName )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.StartDate )
            </td>
            <td>
               @Html.DisplayFor( modelItem => item.EndDate )
            </td>
         </tr>
      }
   </tbody>
</table>